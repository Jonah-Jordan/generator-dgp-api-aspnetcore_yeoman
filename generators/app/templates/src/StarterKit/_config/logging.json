{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "formatter": "StarterKit.Framework.Logging.DigipolisFormatter, StarterKit"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "SerilogDev": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "formatter": "StarterKit.Framework.Logging.DigipolisFormatter, StarterKit"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}

