{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "logstash-application-netloggertestapi-{0:yyyy.MM.dd}",
          "templateName": "netloggertestapi-template",
          "typeName": "AppLogEvent",
          "batchPostingLimit": 50,
          "period": 5,
          "bufferBaseFilename": "C:/Temp/LogDigipolis/netloggertestapi-buffer-application",
          "bufferLogShippingInterval": 5000,
          "restrictedToMinimumLevel": "Information",
          "connectionGlobalHeaders": "",
          "formatter": "MyTestApi.Formatter.DigipolisFormatter, MyTestApi"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}

